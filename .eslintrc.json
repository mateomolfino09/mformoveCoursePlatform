{
  "extends": [
    "./node_modules/standard/eslintrc.json",
    "next/core-web-vitals",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": 0,
    "no-unused-vars": "off",
    "react/no-unescaped-entities": "off",
    "eqeqeq": "off",
    "object-shorthand": "off",
    "spaced-comment": "off",
    "no-unneeded-ternary": "off"
  }

}
